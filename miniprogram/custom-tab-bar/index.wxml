<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange" active-color="green">

  <van-tabbar-item wx:for="{{list}}" wx:key="index" icon="{{item.icon}}" info="{{item.info ? item.info : ''}}" data-path="{{item.pagePath}}">
  {{item.text}}</van-tabbar-item>
 
</van-tabbar>
